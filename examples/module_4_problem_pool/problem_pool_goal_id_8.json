        "@type": "SequenceMetadata",
        "problem_id": 1.0,
        "goal_id": 8.0,
        "goal_text": "The student can compare the size of equal intervals between number lines.",
        "verb": "COMPARE",
        "variables_covered": {
          "fractions": [
            "1/2",
            "1/4"
          ]
        },
        "mastery_tier": "STRETCH",
        "mastery_component": "CONCEPTUAL",
        "mastery_verbs": [
          "COMPARE"
        ]
      },
      "steps": [
        {
          "@type": "Step",
          "dialogue": "Here are two number lines from 0 to 1. One has larger intervals than the other. Which one has the larger intervals?",
          "workspace": {
            "@type": "WorkspaceData",
            "tangibles": [
              {
                "@type": "NumberLine",
                "lcd": 9,
                "is_visible": true,
                "tick_marks": [
                  0,
                  "1/2",
                  1
                ],
                "labelled": [
                  true,
                  true,
                  true
                ]
              },
              {
                "@type": "NumberLine",
                "lcd": 9,
                "is_visible": true,
                "tick_marks": [
                  0,
                  "1/4",
                  "2/4",
                  "3/4",
                  1
                ],
                "labelled": [
                  true,
                  true,
                  true,
                  true,
                  true
                ]
              }
            ]
          },
          "prompt": {
            "@type": "Prompt",
            "text": "Select the number line with larger intervals.",
            "tool": "select",
            "validator": {
              "@type": "SelectionValidator",
              "answer": 0
            },
            "remediations": [
              {
                "@type": "Remediation",
                "id": "light",
                "step": {
                  "@type": "Step",
                  "dialogue": "Not quite. Compare the distance between marks on each [vocab]number line[/vocab]."
                }
              },
              {
                "@type": "Remediation",
                "id": "medium",
                "step": {
                  "@type": "Step",
                  "metadata": {
                    "events": [
                      {
                        "name": "selected_compare",
                        "description": "Compare parts of the selected shape"
                      }
                    ]
                  },
                  "dialogue": "Let's look more carefully. [event:selected_compare] Notice the spacing between the tick marks you selected - now compare that to the spacing on the other [vocab]number line[/vocab]."
                }
              },
              {
                "@type": "Remediation",
                "id": "heavy",
                "step": {
                  "@type": "Step",
                  "metadata": {
                    "events": [
                      {
                        "name": "A_compare",
                        "description": "Compare all the parts of the first or the top shape with equal parts"
                      }
                    ]
                  },
                  "dialogue": "Let me show you how to compare intervals. [event:A_compare] Look at the top [vocab]number line[/vocab] - it has 2 intervals from 0 to 1. Each [vocab]interval[/vocab] is larger because there are fewer of them. The bottom number line has 4 intervals, so each one is smaller. Fewer intervals means larger spacing."
                }
              }
            ],
            "on_correct": {
              "@type": "Step",
              "dialogue": "Correct. Fewer intervals means each [vocab]interval[/vocab] is larger."
            }
          }
        }
      ]
    },
